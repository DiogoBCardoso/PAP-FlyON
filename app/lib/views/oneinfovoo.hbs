<div class="div1">
     <img class="logo" src="/PAPLogo.png">
</div>
<div class="contentor">
    <table class="table table-striped table-light">
        <tr>
            <th>Id InfoVoo</th>
            <th>Id Proposta</th>
            <th>Companhia</th>
            <th>Origem</th>
            <th>Destino</th>
            <th>Data Partida</th>
            <th>Horário Partida</th>
            <th>Data Chegada</th>
            <th>Horário Chegada</th>
        </tr>
        {{#each data}}
            <tr>
                <td>
                    {{id}}
                </td>
                <td>
                <a href="/flyon/proposta/{{idproposta}}">{{idproposta}}</a>
                </td>
                <td>
                    {{companhia}}
                </td>
                <td>
                    {{origem}}
                </td>
                <td>
                    {{destino}}
                </td>
                <td>
                    {{partidadata}}
                </td>
                <td>
                    {{partidatempo}}
                </td>
                <td>
                    {{chegadadata}}
                </td>
                <td>
                    {{chegadatempo}}
                </td>
            </tr>
        {{/each}}
    </table>

    <a href="/flyon/infovoo" class="btn botoes">Voltar para InfoVoos</a>
    <a href="/flyon/" class="btn botoes">Voltar para Menu</a>
    <a href="/flyon/infovoo/{{data.0.id}}/delete" class="btn botoes" id="apagar">Apagar InfoVoo</a>
</div>
<style>
    .botoes
    {
        width: 800px;
        height: 70px;
        font-size: 50px;
        line-height: 100%;
        font-weight: 500;
    }

    #apagar
    {
        background-color: red !important;
    }
</style>