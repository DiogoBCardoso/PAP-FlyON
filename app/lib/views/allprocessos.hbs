<div class="div1">
     <img class="logo" src="/PAPLogo.png">
</div>
<div class="contentor">
    <table class="table table-striped table-light">
        <tr>
            <th>Id Processo</th>
            <th>Origem</th>
            <th>Destino</th>
            <th>Num Adultos</th>
            <th>Check In</th>
            <th>Check Out</th>
            <th>Dist ao Centro</th>      
            <th>Partida</th>
        </tr>
        {{#each data}}
            <tr>
                <td>
                    <a href="/flyon/processo/{{id}}">{{id}}</a>
                </td>
                <td>
                    {{origem}}
                </td>
                <td>
                    {{cidadedestino}}
                </td>
                <td>
                    {{numadultos}}
                </td>
                <td>
                    {{checkin}}
                </td>
                <td>
                    {{checkout}}
                </td>
                
                <td>
                    {{radius}} Km
                </td>
                <td>
                    {{partida}}
                </td>
            </tr>
        {{/each}}
    </table>

    <a href="/flyon/processo/create" class="btn botoes" style="background-color: #4fbd68 !important;">Criar Novo Processo</a>
    <a href="/flyon/" class="btn botoes">Voltar para Menu</a>
</div>
<style>
    .botoes
    {
        width: 800px;
        height: 70px;
        font-size: 50px;
        line-height: 100%;
        font-weight: 500;
    }
</style>